{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "carlansell94/summary-archive",
	"version": "0.1.0",
	"title": "Summary Archive",
	"category": "widgets",
	"icon": "archive",
	"description": "A post archive using HTML summary drop-down elements.",
	"keywords": [ "archive" ],
	"attributes": {
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"showPostCounts": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "qb-summary-archive",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
